<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CollectiveNote" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="android:windowEnableSplitTouch">false</item>
        <item name="android:splitMotionEvents">false</item>
        <item name="colorPrimary">@color/main_color</item>
        <item name="colorPrimaryVariant">@color/main_color_dark</item>

        <item name="colorOnPrimary">@color/white</item>
        <item name="colorControlNormal">@color/color_grey_image</item>


        <item name="colorSecondary">@color/main_color_secondary</item>
        <item name="colorSecondaryVariant">@color/design_default_color_secondary_variant</item>
        <item name="colorOnSecondary">@color/black</item>


        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>



    </style>




</resources>